/* Cross Border Styles */
.footer-section .disclosure {
	 position: relative;
}
.footer-section .disclosure__toggle {
	 white-space: nowrap;
   color: {{ settings.footer_text }};
   border: 1px solid {{ settings.footer_text }};
   background-color: {{ settings.footer_background }};
   margin: 0;
   text-transform: capitalize;
}
.footer-section .disclosure-list {
	 background-color: {{ settings.footer_text }};
	 border: 1px solid {{ settings.footer_background }};
	 bottom: 115%;
	 padding: 11px 0px;
	 position: absolute;
	 display: none;
	 min-height: 92px;
	 max-height: 300px;
	 overflow-y: auto;
	 border-radius: 2px;
   text-transform: capitalize;
}
.footer-section .disclosure-list--visible {
	 display: block;
}
.footer-section .disclosure-list__item {
	 border-bottom: 1px solid transparent;
	 white-space: nowrap;
	 padding: 5px 45px 4px 15px;
	 text-align: left;
}
.footer-section .disclosure-list__item:active {
	 background-color: rgba(128, 128, 128, 0.2);
}
.footer-section .disclosure-list__option {
	 color: {{ settings.footer_background }} !important;
}
.footer-section .disclosure-list__option:focus, .disclosure-list__option:hover {
	 color: {{ settings.footer_text }};
	 border-bottom: 1px solid {{ settings.footer_background }};
}
.footer-section .disclosure-list__item--current .disclosure-list__option {
   color: {{ settings.footer_background }};
	 border-bottom: 1px solid {{ settings.footer_background }} !important;
   font-weight: 900;
}

