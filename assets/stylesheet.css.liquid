nav {
  width: 100%;
  background: {{ settings.navigation }};
  margin-bottom: 0px;
  z-index: 2;
}

ul#main-nav {
  list-style: none;
  text-align: center;
  margin-bottom: 0;
  position: relative;
}

ul#main-nav li {
  display: inline-block;
}

ul#main-nav li:hover {
  background: {{ settings.nav_hover_color }};
}

ul#main-nav li a {
  font-family: {{ nav_font.family}};
  font-weight: {{ nav_font.weight }};
  font-size: {{ settings.nav_size }}px;
  color: {{ settings.nav_color }};
  display: block;
  padding: 0 20px;
  text-transform: {{ settings.navigation_transform}};
}

ul#main-nav li a:hover {
  color: {{ settings.nav_hover_link_color }};
  text-decoration: none;
  border-bottom: none;
}
ul#main-nav li a:visited {
  border-bottom: none;
}
ul#main-nav li.dropdown > a:after {
  font-family: 'FontAwesome';
  content: "\f107";
  margin-left: 10px;
}

ul#main-nav li ul.submenu {
  display: none;
  position: absolute;
  top: 0;
  padding: 10px 0;
  z-index: 5;
  background: {{ settings.dropdown_background }};
  border-bottom: 1px solid {{ settings.nav_border_color }};
  border-left: 1px solid {{ settings.nav_border_color }};
  border-right: 1px solid {{ settings.nav_border_color }};
}

ul#main-nav li:hover ul.submenu{
  display: block;
}

ul#main-nav li ul.submenu li{
  line-height: 34px !important;
  margin: 0;
  padding: 0;
  white-space: nowrap;
  background: transparent !important;
  min-width: 220px;
  display: block;
  position: relative;
}

ul#main-nav li ul.submenu li:hover {
  background: {{ settings.dropdown_hover_background }} !important;
}

ul#main-nav li ul.submenu li a{
  text-align: left;
  font-weight: {{ main_font.weight }};
  text-transform: none !important;
  color: {{ settings.dropdown_link_color }};
  font-size: {{ settings.font_size }}px !important;
  font-family:{{ main_font.family }} !important;
}

ul#main-nav li ul.submenu li a:hover {
  color: {{ settings.dropdown_link_hover_color }};
}

ul.submenu li.nest.has_sub_menu.edge:hover ul.nested {
  left: initial;
  right: 100%;
}

ul#main-nav li ul.submenu li.nest:after {
  position: absolute;
  top: 0px;
  right: 10px;
  font-family: 'FontAwesome';
  content: "\f105";
}

.submenu li.nest:hover > ul.nested {
  display: block;
}

.nested {
  display: none;
  position: absolute;
  left: 100%;
  padding: 10px 0;
  top: -10px;
  background: {{ settings.dropdown_background }};
  border: 1px solid {{ settings.nav_border_color }};
}

/* Megamenu */

ul#main-nav li.dropdown:hover > ul.megamenu {
  display: block;
}

ul.megamenu {
  display: none;
  position: absolute;
  left: 0;
  z-index: 2;
  line-height: initial;
  padding: 10px 10px 30px;
  background: {{ settings.dropdown_background }};
  border: 1px solid {{ settings.nav_border_color }};
}

ul.megamenu h4 {
  padding: 20px 0 5px;
  font-family: {{ nav_font.family}} !important;
  color: {{ settings.dropdown_link_color }};
  font-weight: {{ heading_font.weight }};
  border-bottom: 1px solid {{ settings.dotted_color }};
  margin-bottom: 10px;
  text-align: left;
  font-size: {{ settings.nav_size }}px;
  letter-spacing: initial;
}

ul.megamenu li > ul {
  width: 100%;
}

ul.megamenu li:hover {
  background: transparent !important;
}

ul.megamenu li > ul.mega-stack {
  background: {{ settings.dropdown_background }};
}

ul.megamenu li > ul.mega-stack li {
  line-height: 20px !important;
  text-align: left !important;
  display: block !important;
  clear: left;
}

ul.megamenu li > ul.mega-stack li a {
  padding: 0 !important;
  font-weight: normal !important;
  color: {{ settings.dropdown_link_color }} !important;
  font-size: {{ settings.font_size }}px !important;
  font-family:{{ main_font.family }} !important;
  background: transparent !important;
  text-transform: none !important;
}

ul.megamenu li > ul.mega-stack li a:hover {
  color: {{ settings.dropdown_link_hover_color }} !important;
}

.megamenu .box-ratio {
  margin-top: 20px;
}

ul#main-nav li.dropdown ul.megamenu li.desktop-3:nth-child(5n) {
